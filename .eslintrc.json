{
    "env": {
        "browser": true,
        "es2021": true
    },
  "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "eslint:recommended", "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}





